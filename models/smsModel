const {sequilize} = require("../../config/mysql");
const {DataTypes} = require("sequilize");

const SMS =  sequilize.define(
    "SMS",{
        body:{
            type: DataTypes.STRING,
            allowNull:false
        },
        from:{
            type: DataTypes.STRING,
            allowNull:false
        },
        to:{
            type: DataTypes.STRING,
            allowNull:false
        },
        ordenCompra:{
            type: DataTypes.ENUM,
            allowNull:false
        },
        cliente:{
            type: DataTypes.ENUM,
            allowNull:false
        }
    },{
        timestamps: true
    }
);

module.exports.SMS;